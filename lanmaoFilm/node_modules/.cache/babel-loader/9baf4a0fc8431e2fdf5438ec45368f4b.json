n\">\r\n            <button @touchstart=\"handleToRegister\">注册</button>\r\n        </div>\r\n        <div class=\"register_link\">\r\n           <router-link to=\"/mine/login\">立即登录</router-link>          \r\n           <router-link to=\"/mine/findPassword\">找回密码</router-link>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { messageBox } from '@/components/JS'\r\n\r\nexport default {\r\n    name : 'register',\r\n    data(){\r\n        return {\r\n            email : '',\r\n            username : '',\r\n            password : '',\r\n            verify : '' , //验证码\r\n            verifyInfo : '发送验证码',\r\n            disabled : false\r\n        }\r\n    },\r\n    methods : {\r\n        handleToVerify(){\r\n            if(this.disabled){return;}\r\n            this.axios.get('/api2/users/verify?email=' + this.email ).then((res)=>{\r\n                var status = res.data.status;\r\n                var This = this;\r\n                if( status === 0 ){\r\n                    messageBox({\r\n                        title : '验证码',\r\n                        content : '验证码已发送',\r\n                        ok : '确定',\r\n                        handleOk(){\r\n                            This.countDown();\r\n                        }\r\n                       \r\n                    })\r\n\r\n                }else{\r\n                     messageBox({\r\n                        title : '验证码',\r\n                        content : '验证码发送失败',\r\n                        ok : '确定'\r\n                    })\r\n                    \r\n\r\n                }\r\n            }).catch((err)=>{\r\n                console.log('err',err);\r\n\r\n            });\r\n        },\r\n        handleToRegister(){\r\n            this.axios.post('/api2/users/register',{\r\n                email : this.email,\r\n                username : this.username,\r\n                password : this.password,\r\n                verify : this.verify            \r\n            }).then((res)=>{\r\n                var status = res.data.status;\r\n                var This = this;\r\n                if(status === 0){\r\n                    messageBox({\r\n                        title : '注册',\r\n                        content : '用户注册成功',\r\n                        ok : '确定',\r\n                        handleOk(){\r\n                            This.$router.push('/mine/login');\r\n                        }\r\n                    });\r\n                }\r\n                else{\r\n                    messageBox({\r\n                        title : '注册',\r\n                        content : res.data.msg + '请重新注册',\r\n                        ok : '确定'\r\n                    })\r\n                }\r\n            \r\n            })\r\n        },\r\n        //验证码倒计时\r\n        countDown(){\r\n            this.disabled = true;\r\n            var count = 60;\r\n            var timer = setInterval(()=>{\r\n                count--;\r\n                this.verifyInfo = '剩余' + count + '秒';\r\n                if( count === 0){\r\n                    this.disabled = false;\r\n                    count = 60;\r\n                    this.verifyInfo = \"发送验证码\";\r\n                    clearInterval(timer._id);\r\n                }\r\n\r\n            },1000)\r\n        }\r\n    }\r\n\r\n\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n#content .register_body{ width:100%;}\r\n.register_body .register_text{ width: 100%; height: 40px; border:none; border-bottom: 1px #ccc solid; margin-bottom: 5px; outline: none;text-indent: 2em;}\r\n.register_body .register_btn{ height: 50px; margin:10px;}\r\n.register_body .register_email{ position: relative;}\r\n.register_body .register_email button{ position: absolute; right: 10px; top:10px; height: 30px;background:#e54847;color: #fff; border:0; border-radius: 3px; padding:0 5px;}\r\n.register_body .register_btn button{ width:100%; height: 100%; background:#e54847; border-radius: 3px; border:none; display: block;color:#fff;}\r\n.register_body .register_link{ display: flex; justify-content: space-between;}\r\n.register_body .register_link a{ text-decoration: none; margin:0 5px; font-size: 12px; color:#e54847;}\r\n\r\n</style>\r\n"],"sourceRoot":"src/components/Register"}]}